{
  "name": "auto-repair-shop",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix --ignore-path .gitignore",
    "commit": "git cz"
  },
  "dependencies": {
    "expo": "^51.0.37",
    "expo-av": "~14.0.7",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.27",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@trivago/prettier-plugin-sort-imports": "^3.3.1",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "~5.3.3"
  },
  "private": true,
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "npm run lint:fix",
    "*.{js,ts,tsx,jsx,html,css,md}": "npm run format"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  }
}
